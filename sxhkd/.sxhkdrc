super + Q
  pkill swm

super + Return
  urxvt

super + space
  dmenu_run

super + R
  pkill -USR1 sxhkd

super + Down
 move.sh 0 20 $(pfw)

super + Up
  move.sh 0 -20 $(pfw)

super + Right
  move.sh 20 0 $(pfw)

super + Left
  move.sh wmv -20 0 $(pfw)

super + alt + Up
  resize.sh 0 -20 $(pfw)

super + alt + Down
  resize.sh 0 20 $(pfw)

super + alt + Right
  resize.sh 20 0 $(pfw)

super + alt + Left
  resize.sh -20 0 $(pfw)

super + H
  teleport.sh 20 20 910 1024 $(pfw)

super + L
  teleport.sh 980 20 910 1024 $(pfw)

super + I
  teleport.sh 20 20 1860 1024 $(pfw)

super + f
  fullscreen.sh $(pfw)

super + !button{1,3}
  {wmv, wrs} -a %1 %1 $(pfw)

XF86MonBrightnessUp
  xbacklight -inc 10

XF86MonBrightnessDown
  xbacklight -dec 10

super + n
  focus.sh next

super + p
  focus.sh prev

super + shift + {1,2,3,4,5}
  groups.sh -s {1,2,3,4,5} $(pfw)

super + {1,2,3,4,5}
  groups.sh -M {1,2,3,4,5}

super + shift + d
  groups.sh -c $(pfw)
